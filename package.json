{
  "name": "tailor-dev-kit",
  "version": "1.0.0",
  "description": "Tailor Development Kit - Workspace Root",
  "type": "module",
  "packageManager": "pnpm@10.8.0",
  "scripts": {
    "test": "turbo run test",
    "test:root": "vitest run",
    "test:root:update-expects": "tsx tests/scripts/generate_expected_files.ts",
    "gen": "tailor-sdk generate",
    "gen:perf": "dotenvx run -- tailor-sdk generate",
    "apply": "TAILOR_SDK_OUTPUT_DIR=out tailor-sdk apply",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "lint:root": "eslint --cache .",
    "lint:fix": "turbo run lint:fix",
    "lint:fix:root": "eslint --cache . --fix",
    "format": "turbo run format",
    "format:root": "prettier --write .",
    "format:check": "turbo run format:check",
    "format:check:root": "prettier --check ."
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.45.1",
    "@eslint/js": "^9.29.0",
    "@tailor-platform/tailor-sdk": "workspace:^",
    "@types/node": "^22.15.3",
    "date-fns": "^4.1.0",
    "dir-compare": "^4.2.0",
    "es-toolkit": "^1.38.0",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "globals": "^16.2.0",
    "multiline-ts": "^4.0.1",
    "prettier": "3.5.3",
    "rolldown": "1.0.0-beta.14",
    "rollup": "^4.42.0",
    "rollup-plugin-esbuild-minify": "^1.3.0",
    "ts-morph": "^26.0.0",
    "turbo": "^2.5.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.1",
    "vitest": "^3.1.4"
  },
  "engines": {
    "node": ">= 22.14.0"
  }
}
