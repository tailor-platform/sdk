# Performance measurement configuration

# Log level for performance tracking
# - "summary": Track performance with summary output
# - "detailed": Track performance with detailed output
# - undefined (not set): Disable performance tracking
TAILOR_PERFORMANCE_LOG_LEVEL=detailed

# Minimum execution time threshold in milliseconds to track
# Only methods that take longer than this threshold will be tracked
TAILOR_PERFORMANCE_THRESHOLD_MS=10

# Report type (console/json)
# - "console": Output performance report to console
# - "json": Save performance report as JSON file
TAILOR_PERFORMANCE_REPORT_TYPE=console

# Directory path for JSON reports (only used when REPORT_TYPE=json)
# JSON reports will be saved with filename: performance-{timestamp}.json
TAILOR_PERFORMANCE_REPORT_PATH=./performance-reports
