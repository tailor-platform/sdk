{
  "name": "@examples/basic",
  "version": "1.0.0",
  "description": "Basic example for Tailor SDK usage",
  "type": "module",
  "scripts": {
    "gen": "tailor-sdk generate -c tailor.config.ts",
    "gen:watch": "tailor-sdk generate -c tailor.config.ts --watch",
    "apply": "tailor-sdk apply -c tailor.config.ts",
    "apply:operator": "TAILOR_SDK_USE_OPERATOR_API=true tailor-sdk apply -c tailor.config.ts",
    "test": "pnpm test:generator",
    "test:all": "pnpm test:generator:prepare && vitest",
    "test:generator": "pnpm test:generator:prepare && vitest --project generator",
    "test:generator:prepare": "tsx tests/scripts/generate_files.ts actual",
    "test:generator:update-expects": "tsx tests/scripts/generate_files.ts",
    "test:e2e": "vitest --project e2e",
    "lint": "eslint --cache .",
    "lint:fix": "eslint --cache . --fix --quiet",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@tailor-platform/tailor-proto": "workspace:^",
    "@tailor-platform/tailor-sdk": "workspace:^",
    "date-fns": "^4.1.0",
    "kysely": "0.27.6",
    "sqlstring": "^2.3.3"
  },
  "devDependencies": {
    "@connectrpc/connect": "^2.0.3",
    "@connectrpc/connect-node": "^2.0.3",
    "@types/node": "^22.15.3",
    "@types/sqlstring": "^2.3.2",
    "dir-compare": "^4.2.0",
    "graphql": "^16.11.0",
    "graphql-request": "^7.2.0",
    "tsx": "^4.19.2",
    "typescript": "^5.8.3",
    "vitest": "^3.1.4",
    "vitest-mock-extended": "^2.0.2"
  },
  "engines": {
    "node": ">= 22.14.0"
  }
}
