{
  "name": "@tailor-platform/create-tailor-sdk",
  "version": "0.0.0",
  "description": "A CLI tool to quickly create a new Tailor SDK project",
  "type": "module",
  "bin": "dist/index.js",
  "files": [
    "dist",
    "templates",
    "templates/*/.gitignore"
  ],
  "scripts": {
    "build": "tsdown",
    "lint": "eslint --cache .",
    "lint:fix": "eslint --cache --fix .",
    "typecheck": "tsc --noEmit",
    "prepublish": "pnpm run build"
  },
  "dependencies": {
    "@clack/prompts": "0.11.0",
    "citty": "0.1.6",
    "execa": "9.6.0",
    "picocolors": "1.1.1",
    "pkg-types": "2.3.0"
  },
  "devDependencies": {
    "@eslint/js": "9.36.0",
    "@types/node": "22.18.6",
    "eslint": "9.36.0",
    "tsdown": "0.15.5",
    "typescript": "5.9.2",
    "typescript-eslint": "8.45.0"
  }
}
