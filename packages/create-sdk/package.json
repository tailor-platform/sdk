{
  "name": "@tailor-platform/create-sdk",
  "version": "1.2.0",
  "description": "A CLI tool to quickly create a new Tailor Platform SDK project",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tailor-platform/sdk.git",
    "directory": "packages/create-sdk"
  },
  "bin": "dist/index.js",
  "files": [
    "CHANGELOG.md",
    "dist",
    "LICENSE",
    "README.md",
    "templates"
  ],
  "type": "module",
  "scripts": {
    "build": "tsdown",
    "lint": "oxlint . && eslint --cache .",
    "lint:fix": "oxlint --fix . && eslint --cache --fix .",
    "typecheck": "tsc --noEmit",
    "prepublish": "node scripts/prepare-templates.js && pnpm run build"
  },
  "dependencies": {
    "@clack/prompts": "0.11.0",
    "citty": "0.1.6",
    "execa": "9.6.1",
    "picocolors": "1.1.1",
    "pkg-types": "2.3.0"
  },
  "devDependencies": {
    "@eslint/js": "9.39.2",
    "@types/node": "24.10.4",
    "eslint": "9.39.2",
    "eslint-plugin-oxlint": "1.34.0",
    "oxlint": "1.34.0",
    "tsdown": "0.18.4",
    "typescript": "5.9.3",
    "typescript-eslint": "8.51.0"
  }
}
