{
  "name": "@tailor-platform/create-sdk",
  "version": "0.7.6",
  "description": "A CLI tool to quickly create a new Tailor Platform SDK project",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/tailor-platform/sdk.git",
    "directory": "packages/create-sdk"
  },
  "bin": "dist/index.js",
  "files": [
    "dist",
    "templates",
    "templates/*/.gitignore",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsdown",
    "lint": "eslint --cache .",
    "lint:fix": "eslint --cache --fix .",
    "typecheck": "tsc --noEmit",
    "prepublish": "node scripts/set-sdk-version.js && pnpm run build"
  },
  "dependencies": {
    "@clack/prompts": "0.11.0",
    "citty": "0.1.6",
    "execa": "9.6.0",
    "picocolors": "1.1.1",
    "pkg-types": "2.3.0"
  },
  "devDependencies": {
    "@eslint/js": "9.39.1",
    "@types/node": "22.19.0",
    "eslint": "9.39.1",
    "tsdown": "0.15.6",
    "typescript": "5.9.3",
    "typescript-eslint": "8.46.3"
  }
}
