{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "ignoreExportsUsedInFile": {
    "interface": true,
    "type": true
  },
  "tags": ["-lintignore"],
  "ignore": ["scripts/**", "e2e/fixtures/**"],
  "ignoreDependencies": [
    "cross-env",
    "oxlint",
    "oxlint-tsgolint",
    "@typescript/native-preview",
    "tsx",
    "eslint-plugin-import"
  ],
  "ignoreBinaries": ["vitest", "tsdown", "cross-env", "oxlint", "eslint", "tsc", "tsgo", "knip"]
}
